<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/home_bg">
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.LoadingPage 
        android:id="@+id/id_result_loading_page"
		android:layout_width="1280dp"
		android:layout_height="720dp"
		android:layout_marginLeft="0dp"
 		android:layout_marginTop="0dp"
        android:visibility="invisible"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.LoadingPage> 
    
    <TextView 
        android:id="@+id/id_result_title"
 		android:layout_width="wrap_content"
 		android:layout_height="wrap_content"
 		android:layout_marginLeft="130dp"
 		android:layout_marginTop="40dp"
 		android:gravity="left"
 		android:textSize="@dimen/font_24"
 		android:textColor="@color/white"
 		android:singleLine="true"
 		android:ellipsize="end"
 		android:text="@string/home_search"/>
    
    <TextView 
        android:id="@+id/id_result_keyword_prefix"
 		android:layout_width="wrap_content"
 		android:layout_height="wrap_content"
 		android:layout_alignTop="@+id/id_result_title"
 		android:layout_alignBottom="@+id/id_result_title"
 		android:layout_toRightOf="@+id/id_result_title"
 		android:layout_marginLeft="0dp"
 		android:gravity="left"
 		android:textSize="@dimen/font_24"
 		android:textColor="@color/white"
 		android:singleLine="true"
 		android:ellipsize="end"
 		android:text=""/>
    
    <TextView 
        android:id="@+id/id_result_keyword"
 		android:layout_width="wrap_content"
 		android:layout_height="wrap_content"
 		android:layout_alignTop="@+id/id_result_keyword_prefix"
 		android:layout_alignBottom="@+id/id_result_keyword_prefix"
 		android:layout_toRightOf="@+id/id_result_keyword_prefix"
 		android:layout_marginLeft="0dp"
 		android:gravity="left"
 		android:textSize="@dimen/font_24"
 		android:textColor="@color/blue"
 		android:singleLine="true"
 		android:ellipsize="end"/>
    
    <TextView 
        android:id="@+id/id_result_keyword_suffix"
 		android:layout_width="wrap_content"
 		android:layout_height="wrap_content"
 		android:layout_alignTop="@+id/id_result_keyword"
 		android:layout_alignBottom="@+id/id_result_keyword"
 		android:layout_toRightOf="@+id/id_result_keyword"
 		android:layout_marginLeft="0dp"
 		android:gravity="left"
 		android:textSize="@dimen/font_24"
 		android:textColor="@color/white"
 		android:singleLine="true"
 		android:ellipsize="end"
 		android:text=""/>
    
    <TextView 
        android:id="@+id/id_result_number"
 		android:layout_width="wrap_content"
 		android:layout_height="wrap_content"
 		android:layout_alignTop="@+id/id_result_keyword_suffix"
 		android:layout_alignBottom="@+id/id_result_keyword_suffix"
 		android:layout_toRightOf="@+id/id_result_keyword_suffix"
 		android:layout_marginLeft="0dp"
 		android:gravity="center"
 		android:textSize="@dimen/font_24"
 		android:textColor="@color/white"
 		android:singleLine="true"
 		android:ellipsize="end"
 		android:visibility="invisible"/>
    
    <ImageView  
        android:id="@+id/id_result_focus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/app_name"
        android:src="@drawable/focus"
        android:visibility="invisible"
        android:scaleType="fitXY"/>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item0"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:nextFocusRight="@+id/id_result_item1"
        android:nextFocusDown="@+id/id_result_item5"
        android:layout_marginLeft="154dp"
        android:layout_marginTop="130dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item1"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item0"
        android:layout_alignTop="@+id/id_result_item0"
        android:nextFocusDown="@+id/id_result_item6"
        android:nextFocusLeft="@+id/id_result_item0"
        android:nextFocusRight="@+id/id_result_item2"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item2"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item1"
        android:layout_alignTop="@+id/id_result_item1"
        android:nextFocusDown="@+id/id_result_item7"
        android:nextFocusLeft="@+id/id_result_item1"
        android:nextFocusRight="@+id/id_result_item3"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item3"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item2"
        android:layout_alignTop="@+id/id_result_item2"
        android:nextFocusDown="@+id/id_result_item8"
        android:nextFocusLeft="@+id/id_result_item2"
        android:nextFocusRight="@+id/id_result_item4"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item4"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item3"
        android:layout_alignTop="@+id/id_result_item3"
        android:nextFocusDown="@+id/id_result_item9"
        android:nextFocusLeft="@+id/id_result_item3"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item5"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_below="@+id/id_result_item0"
        android:layout_alignLeft="@+id/id_result_item0"
        android:nextFocusUp="@+id/id_result_item0"
        android:nextFocusRight="@+id/id_result_item6"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="32dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item6"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item5"
        android:layout_alignTop="@+id/id_result_item5"
        android:nextFocusUp="@+id/id_result_item1"
        android:nextFocusLeft="@+id/id_result_item5"
        android:nextFocusRight="@+id/id_result_item7"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item7"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item6"
        android:layout_alignTop="@+id/id_result_item6"
        android:nextFocusUp="@+id/id_result_item2"
        android:nextFocusLeft="@+id/id_result_item6"
        android:nextFocusRight="@+id/id_result_item8"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item8"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item7"
        android:layout_alignTop="@+id/id_result_item7"
        android:nextFocusUp="@+id/id_result_item3"
        android:nextFocusLeft="@+id/id_result_item7"
        android:nextFocusRight="@+id/id_result_item9"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.VodItem
        android:id="@+id/id_result_item9"
        android:layout_width="155dp"
        android:layout_height="214dp"
        android:layout_toRightOf="@+id/id_result_item8"
        android:layout_alignTop="@+id/id_result_item8"
        android:nextFocusUp="@+id/id_result_item4"
        android:nextFocusLeft="@+id/id_result_item8"
        android:layout_marginLeft="49dp"
        android:layout_marginTop="0dp"
        android:focusable="true">
    </com.szgvtv.ead.app.taijietemplates.ui.view.VodItem>
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.NoContent 
        android:id="@+id/id_result_no_content"
		android:layout_width="1280dp"
		android:layout_height="720dp"
		android:layout_marginLeft="0dp"
 		android:layout_marginTop="0dp"
        android:visibility="invisible">
    </com.szgvtv.ead.app.taijietemplates.ui.view.NoContent> 
    
    <com.szgvtv.ead.app.taijietemplates.ui.view.DotGroup 
        android:id="@+id/id_result_dot"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
        android:visibility="visible">
    </com.szgvtv.ead.app.taijietemplates.ui.view.DotGroup> 

</RelativeLayout>