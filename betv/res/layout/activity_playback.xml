<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="1280dp"
    android:layout_height="720dp"
    android:background="@color/transparent" >

    <!-- 播放器 -->
    <com.szgvtv.ead.app.taijietemplates.ui.view.GVideoView
        android:id="@+id/id_playback_videoview"
        android:layout_width="1280dp"
        android:layout_height="720dp"> 
    </com.szgvtv.ead.app.taijietemplates.ui.view.GVideoView>
        
    <!-- 加载广告 -->
    <com.szgvtv.ead.app.taijietemplates.ui.view.LoadingVideo 
        android:id="@+id/id_playback_loading_video"
		android:layout_width="1280dp"
		android:layout_height="720dp"
		android:layout_marginLeft="0dp"
 		android:layout_marginTop="0dp"
        android:visibility="invisible">
    </com.szgvtv.ead.app.taijietemplates.ui.view.LoadingVideo>
    
    <!-- 播放进度 -->
    <com.szgvtv.ead.app.taijietemplates.ui.view.ProgressPlayback 
        android:id="@+id/id_playback_progress_playback"
		android:layout_width="1068dp"
		android:layout_height="126dp"
		android:layout_marginLeft="106dp"
        android:layout_marginTop="554dp"
		android:visibility="invisible">
    </com.szgvtv.ead.app.taijietemplates.ui.view.ProgressPlayback> 
    
    <!-- 音量进度 -->
    <com.szgvtv.ead.app.taijietemplates.ui.view.ProgressVolume 
        android:id="@+id/id_playback_progress_volume"
		android:layout_width="1068dp"
		android:layout_height="126dp"
		android:layout_marginLeft="106dp"
        android:layout_marginTop="554dp"
		android:visibility="invisible">
    </com.szgvtv.ead.app.taijietemplates.ui.view.ProgressVolume> 
    
    <!-- 流媒体维护 -->
    <ImageView
        android:id="@+id/id_ppp_preserve"
        android:layout_width="1280dp"
        android:layout_height="720dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:scaleType="fitXY"
        android:contentDescription="@string/app_name" />
    
</RelativeLayout>
